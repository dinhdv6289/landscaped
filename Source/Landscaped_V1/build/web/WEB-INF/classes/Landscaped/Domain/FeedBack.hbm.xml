<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="Landscaped" name="Landscaped.Domain.FeedBack" table="FeedBack">
        <id name="feedBackId" type="int">
            <column name="FeedBackId"/>
            <generator class="identity" />
        </id>
        <property name="fullName" type="string">
            <column length="100" name="FullName"/>
        </property>
        <property name="address" type="string">
            <column length="100" name="Address"/>
        </property>
        <property name="email" type="string">
            <column length="100" name="Email"/>
        </property>
        <property name="phone" type="string">
            <column length="100" name="Phone"/>
        </property>
        <property name="title" type="string">
            <column length="100" name="Title"/>
        </property>
        <property name="content" type="string">
            <column name="Content"/>
        </property>
        <property name="dateCreation" type="timestamp">
            <column name="DateCreation"/>
        </property>
    </class>
</hibernate-mapping>

